{
  "name": "gekko-browser",
  "productName": "Gekko Browser",
  "version": "1.0.0",
  "description": "A modern, sleek web browser with GKP and GKPS protocol support",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [
    "browser",
    "electron",
    "gkp",
    "gekko"
  ],
  "author": {
    "name": "SpelGekko",
    "email": "spelgekko@gmail.com"
  },
  "license": "MIT",
  "repository": "https://github.com/SpelGekko/Gekko-Browser-Electron/releases",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appID": "com.thorvaldrovers.gekkobrowser",
    "productName": "Gekko Browser",
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico",
      "directories": {
        "output": "dist/win"
      }
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@material-design-icons/svg": "^0.14.15",
    "custom-electron-titlebar": "^4.2.8",
    "electron-log": "^5.4.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "simple-icons": "^15.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "36.3.2",
    "electron-builder": "^26.0.12"
  }
}
